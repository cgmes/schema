<VsConverterShape.uv> rdf:type sh:PropertyShape ;
	sh:path <VsConverter.uv> ;
	sh:datatype xsd:double ;
	sh:minCount "1"^^xsd:integer ;
	sh:maxCount "1"^^xsd:integer ;
	sh:name "Uv"@en ;
